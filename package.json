{
  "name": "sp-pnp-node",
  "description": "SharePoint JavaScript Core Library (PnP JS Core) wrapper helper for Node.js",
  "version": "2.1.6",
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "scripts": {
    "build": "npm run tslint && npm run clean && npm run tsc",
    "tslint": "tslint -p .",
    "tsc": "tsc -p .",
    "clean": "rimraf ./dist",
    "test": "ts-node ./test/init && mocha --opts test/mocha.opts || ECHO.",
    "test:ci": "mocha --opts test/mocha.opts --ci || ECHO.",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "sharepoint",
    "rest",
    "sp-pnp-js",
    "wrapper",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/sp-pnp-node.git"
  },
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koltyakov/sp-pnp-node/issues"
  },
  "homepage": "https://github.com/koltyakov/sp-pnp-node#readme",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "node-sp-auth-config": "^2.6.4",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@pnp/pnpjs": "^1.3.2",
    "@types/chai": "^4.1.7",
    "@types/colors": "^1.2.1",
    "@types/core-js": "^2.5.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.3.4",
    "@types/request": "2.48.1",
    "chai": "^4.2.0",
    "colors": "^1.3.3",
    "cpass": "^2.1.0",
    "dotenv": "^8.0.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "sp-request": "^2.1.3",
    "ts-node": "^8.1.1",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  },
  "snyk": true
}
