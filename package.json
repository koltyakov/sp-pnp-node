{
  "name": "sp-pnp-node",
  "description": "SharePoint JavaScript Core Library (PnP JS Core) wrapper helper for Node.js",
  "version": "2.2.1",
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "scripts": {
    "build": "npm run tslint && npm run clean && npm run tsc",
    "tslint": "tslint -p .",
    "tsc": "tsc -p .",
    "clean": "rimraf ./dist",
    "test:init": "ts-node ./test/init --init",
    "test": "npm run test:init && mocha --config ./test/.mocharc.json || ECHO.",
    "test:ci": "mocha --config ./test/.mocharc.json --ci || ECHO."
  },
  "keywords": [
    "sharepoint",
    "rest",
    "PnPjs",
    "wrapper",
    "nodejs",
    "auth"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/sp-pnp-node.git"
  },
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koltyakov/sp-pnp-node/issues"
  },
  "homepage": "https://github.com/koltyakov/sp-pnp-node#readme",
  "dependencies": {
    "crc-32": "^1.2.0",
    "node-fetch": "^2.6.0",
    "node-sp-auth-config": "^2.9.3"
  },
  "devDependencies": {
    "@pnp/pnpjs": "^1.3.10",
    "@types/chai": "^4.2.11",
    "@types/colors": "^1.2.1",
    "@types/core-js": "^2.5.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.31",
    "@types/node-fetch": "^2.5.5",
    "@types/request": "2.48.4",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "cpass": "^2.3.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "sp-request": "^2.1.3",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  }
}
